<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.voelog">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".TelaCartaoEmbarque"
            android:parentActivityName=".MainActivity2" />
        <activity android:name=".CompreAqui"
                android:parentActivityName=".MainActivity2"/>

        <activity
            android:name=".MainActivity"
            android:parentActivityName=".Login" />
        <activity android:name=".ListaCliente" />
        <activity android:name=".MainActivity2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Check_in"
                android:parentActivityName=".MainActivity2"/>
        <activity
            android:name=".CartaoEmbarqueList"
            android:parentActivityName=".MainActivity2" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>